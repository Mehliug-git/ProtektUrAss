{

    "manifest_version": 2,
    "name": "ProtektUrAss",
    "version": "3",
    "description": "Ouais on verra ca plus tard.",
    "permissions": [
      "activeTab",
      "storage",
      "webRequest",
      "webRequestBlocking",
      "webNavigation",
      "<all_urls>"
    ],
    "icons": {
      "16": "peche.png", 
      "48": "peche.png",  
      "128": "peche.png" 
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js", "punycode.js"],
        "run_at": "document_start"
      } 
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "content_security_policy": "script-src 'self' 'unsafe-inline' 'sha256-9VojYMXme87p0ebCbfqR7Xy2dWS7L70SSh3lPlrRbd4=' 'sha256-iOrzVYgBsYh9oAub0h3O5JU/B4Gocjb0M4IR3FRsiSk=' 'sha256-PXcEYG4M52bxkrSyuKZc3M/z9OiZOTAksPL9nWEYWZc=' 'sha256-VvCvyTsdVmEzVVvE16/8oV4zu6X4i0dtgfpARNSnQSk=' 'sha256-JNw6nG/T+XeqJdx/4CuyN1IBmZHbDYiuD0UEzfmZTAs='; object-src 'self'"

  }
  